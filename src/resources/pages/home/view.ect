<% extend 'layout' %>

<% block 'head-end' :%>
<link rel="stylesheet" href="<%= @__asset('home.css') %>">
<% end %>

<% block 'body-end' :%>
<script src="<%= @__asset('home.js') %>"></script>
<% end %>

<div class="room-cards">
  <% for asset in @assets : %>
    <% include '../elements/card', {
      __asset: @__asset,
      room: asset
    } %>
  <% end %>
</div>
